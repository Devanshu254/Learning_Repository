// Q1. 
#include<bits/stdc++.h>
using namespace std;
int main() {
    string str;
    cin>>str;
    set<int> s;
    for(char c: str) {
        s.insert(c);
    }
    int size = s.size();
    if(size % 2 == 0) {
        cout<<"CHAT WITH HER!"<<endl;
    }else {
        cout<<"IGNORE HIM!"<<endl;
    }
    return 0;
}
