// Time Complexity: O(N^2).
// Inplace and stable.

#include<bits/stdc++.h>
using namespace std;
void InsertionSort(int arr[], int n) {
    for(int i=0;i<n;i++) {
        int key = arr[i];
        int j = i-1;
        while(j>=0 && arr[j] > key) {
            arr[j+1] = arr[j];
            j--;
        }
        arr[j+1] = key;
    }
} 
void printArray(int arr[], int n) {
    for(int i=0;i<n;i++) {
        cout<<arr[i]<<" ";
    }
}
int main() {
    int arr[] = {64, 25, 12, 22, 11};
    int N = sizeof(arr)/sizeof(arr[0]);
    InsertionSort(arr, N);
    printArray(arr, N);
    return 0;
}
